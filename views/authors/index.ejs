<!DOCTYPE html>
<html>
<head>
	<title>New Post Page</title>
</head>
<body>
<nav>
	<a href="/">Home</a>
	<a href="/authors">Authors</a>
	<a href="/authors/new">New Author</a>
	<a href="/posts">Posts</a>
	<a href="/posts/new">New Post</a>
</nav>

<h1>This page will show all the authors</h1>
<% authorsList.forEach(function(author) { %>
<div>
  <a href="/authors/<%= author.id %>"><%= author.firstName %></a>
  <div>
    <%= author.lastName %>
  </div>
</div>
<% }) %>


</body>
</html>