<!DOCTYPE html>
<html>
<head>
	<title>New Post Page</title>
</head>
<body>
<nav>
	<a href="/">Home</a>
	<a href="/authors">Authors</a>
	<a href="/authors/new">New Author</a>
	<a href="/posts">Posts</a>
	<a href="/posts/new">New Post</a>
</nav>

	<h1>This page will show all the posts</h1>

<% postsList.forEach(function(post) { %>
<div>
  <a href="/posts/<%= post.id %>"><%= post.title %></a>
  <div>
    <%= post.content %>
  </div>
</div>
<% }) %>
</body>
</html>